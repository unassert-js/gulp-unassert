{
  "name": "gulp-unassert",
  "description": "Gulp plugin for unassert: Encourages programming with assertions by providing tools to compile them away",
  "version": "2.0.0",
  "author": {
    "name": "Takuto Wada",
    "email": "takuto.wada@gmail.com",
    "url": "https://github.com/twada"
  },
  "bugs": "https://github.com/unassert-js/gulp-unassert/issues",
  "dependencies": {
    "acorn": "^7.0.0",
    "bufferstreams": "^3.0.0",
    "convert-source-map": "^1.1.2",
    "escodegen": "^1.10.0",
    "multi-stage-sourcemap": "^0.3.1",
    "plugin-error": "^1.0.0",
    "through2": "^3.0.0",
    "unassert": "^2.0.0",
    "vinyl-sourcemaps-apply": "^0.2.0"
  },
  "devDependencies": {
    "espower-loader": "^1.0.1",
    "event-stream": "^4.0.0",
    "intelli-espower-loader": "^1.0.1",
    "mocha": "^7.0.0",
    "power-assert": "^1.4.1",
    "semistandard": "^14.2.0",
    "snazzy": "^8.0.0",
    "vinyl": "^2.0.0"
  },
  "files": [
    "LICENSE-MIT",
    "CHANGELOG.md",
    "README.md",
    "index.js"
  ],
  "homepage": "http://github.com/unassert-js/gulp-unassert",
  "keywords": [
    "DbC",
    "assert",
    "assertion",
    "gulpplugin",
    "unassert"
  ],
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "http://github.com/unassert-js/gulp-unassert.git"
  },
  "scripts": {
    "fmt": "semistandard --fix index.js 'test/*.js'",
    "lint": "semistandard --verbose index.js 'test/*.js' | snazzy",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "mocha --require intelli-espower-loader"
  },
  "semistandard": {
    "globals": [
      "describe",
      "context",
      "beforeEach",
      "it"
    ]
  }
}
